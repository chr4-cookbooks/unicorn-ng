#
# Generated by Chef for <%= node['fqdn'] %>.
# Local modifications will be overwritten.
#

<% if @prescript %>
<%= @prescript %>
<% end -%>

<% if @worker_processes %>
worker_processes <%= @worker_processes.inspect %>
<% end -%>

<% if @user %>
user <%= @user.inspect %>
<% end -%>

<% if @working_directory %>
working_directory <%= @working_directory.inspect %>
<% end -%>

<% if @listen %>
listen <%= @listen.inspect %> <%= @backlog ? ", :backlog => #{@backlog.inspect}" : '' %>
<% end -%>

<% if @pid %>
pidfile = <%= @pid.inspect %>
pid pidfile
<% end -%>

<% if @timeout %>
timeout <%= @timeout.inspect %>
<% end -%>

<% if @stderr_path %>
stderr_path <%= @stderr_path.inspect %>
<% end -%>

<% if @stdout_path %>
stdout_path <%= @stdout_path.inspect %>
<% end -%>

<% if defined?(@preload_app) %>
preload_app <%= @preload_app.to_s %>
<% end -%>

<% if @before_fork %>
before_fork do |server, worker|
<%= @before_fork %>
end
<% end -%>

<% if @after_fork %>
after_fork do |server, worker|
<%= @after_fork %>
end
<% end -%>
